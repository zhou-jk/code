{
    "env": {
        "myCompilerPath": "C:/Compiler/MinGW64",
        "myIncludePath1": "${myCompilerPath}/lib/gcc/x86_64-w64-mingw32/8.1.0",
        "myIncludePath2": "${myIncludePath1}/include/c++"
    },
    "configurations": [
        {
            "name": "Win32",
            "compilerPath": "${myCompilerPath}/bin/g++.exe",
            "includePath": [
                "${myIncludePath2}",
                "${myIncludePath2}/x86_64-w64-mingw32",
                "${myIncludePath2}/mingw32",
                "${myIncludePath2}/backward",
                "${myIncludePath1}/include",
                "${myIncludePath1}/include-fixed",
                "${myCompilerPath}/x86_64-w64-mingw32/include",
                "${myCompilerPath}/include"
            ],
            "browse": {
                "path": [
                    "${myIncludePath2}",
                    "${myIncludePath2}/x86_64-w64-mingw32",
                    "${myIncludePath2}/mingw32",
                    "${myIncludePath2}/backward",
                    "${myIncludePath1}/include",
                    "${myIncludePath1}/include-fixed",
                    "${myCompilerPath}/x86_64-w64-mingw32/include",
                    "${myCompilerPath}/include"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "intelliSenseMode": "gcc-x64",
            "cStandard": "c99",
            "cppStandard": "c++98"
        }
    ],
    "version": 4
}